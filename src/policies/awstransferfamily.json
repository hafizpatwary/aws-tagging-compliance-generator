{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denytransfer",
      "Effect": "Deny",
      "Action": [
        "transfer:CreateConnector",
        "transfer:CreateProfile",
        "transfer:CreateServer",
        "transfer:CreateWorkflow",
        "transfer:ImportCertificate"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}