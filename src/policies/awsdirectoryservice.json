{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyds",
      "Effect": "Deny",
      "Action": [
        "ds:ConnectDirectory",
        "ds:CreateDirectory",
        "ds:CreateIdentityPoolDirectory",
        "ds:CreateMicrosoftAD"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}