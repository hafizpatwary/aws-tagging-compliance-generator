{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyevidently",
      "Effect": "Deny",
      "Action": [
        "evidently:CreateExperiment",
        "evidently:CreateFeature",
        "evidently:CreateLaunch",
        "evidently:CreateProject",
        "evidently:CreateSegment"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}