{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyguardduty",
      "Effect": "Deny",
      "Action": [
        "guardduty:CreateDetector",
        "guardduty:CreateIPSet",
        "guardduty:CreateThreatIntelSet"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}