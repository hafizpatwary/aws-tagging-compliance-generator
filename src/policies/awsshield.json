{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyshield",
      "Effect": "Deny",
      "Action": [
        "shield:CreateProtection",
        "shield:CreateProtectionGroup"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}