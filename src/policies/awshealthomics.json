{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyomics",
      "Effect": "Deny",
      "Action": [
        "omics:CreateReferenceStore",
        "omics:CreateRunGroup",
        "omics:CreateSequenceStore",
        "omics:CreateWorkflow",
        "omics:StartRun"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}