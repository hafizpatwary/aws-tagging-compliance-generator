{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyram",
      "Effect": "Deny",
      "Action": [
        "ram:CreatePermission",
        "ram:CreateResourceShare",
        "ram:GetResourceShares"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}