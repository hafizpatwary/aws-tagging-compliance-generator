{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denywisdom",
      "Effect": "Deny",
      "Action": [
        "wisdom:CreateAssistant",
        "wisdom:CreateKnowledgeBase",
        "wisdom:TagResource"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}