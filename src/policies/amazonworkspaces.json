{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denyworkspaces",
      "Effect": "Deny",
      "Action": [
        "workspaces:CreateConnectionAlias",
        "workspaces:CreateIpGroup",
        "workspaces:CreateTags",
        "workspaces:DeleteTags"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}