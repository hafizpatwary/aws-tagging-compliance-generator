{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denystoragegateway",
      "Effect": "Deny",
      "Action": [
        "storagegateway:ActivateGateway",
        "storagegateway:CreateTapePool"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}