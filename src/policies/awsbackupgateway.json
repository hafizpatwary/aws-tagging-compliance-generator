{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Denybackup-gateway",
      "Effect": "Deny",
      "Action": [
        "backup-gateway:CreateGateway",
        "backup-gateway:ImportHypervisorConfiguration"
      ],
      "Resource": "*",
      "Condition": {
        "Null": {
          "aws:RequestTag/team": "true",
          "aws:RequestTag/cost-centre": "true"
        }
      }
    }
  ]
}